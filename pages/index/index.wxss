/**index.wxss**/

.container{
  position:fixed;
  width: 100%;
  height:100%;
  background-color: #D6FFE7;
  /* background-size: cover; */
}

.container view:nth-child(4){
  position: absolute;
  bottom:-100rpx;
  left:0;
}
.container view:nth-child(5){
  position: absolute;
  top:0;
  left:0;
}
.container view:nth-child(6){
  position: absolute;
  bottom:-40rpx;
  right:0;
}

.apply{
  width: 350rpx;
  height: 350rpx;
  margin-top: 50rpx;
  border-radius: 175rpx;
  text-align:center;
  line-height: 350rpx;
  font-size: larger;
  background-color: #C2F9FF;
}
.apply:checked{
  box-shadow:inset 0px 2px 4px 0px rgba(0, 0, 0, 0.25);
}
.title{
  font-size: x-large;
  
}
.query{
  width: 320rpx;
  height: 156rpx;
  margin-top: 50rpx;
  text-align: center;
  line-height: 156rpx;
  opacity: 1;
  background: rgba(255, 250, 207, 1);
  border: 2rpx solid rgba(255, 255, 255, 1);            
  box-shadow: 0rpx 4rpx 8rpx 0rpx rgba(0, 0, 0, 0.25);
}

